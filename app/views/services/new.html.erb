<%= form_for(:service, :url => service_path) do |f| %>
	<div class="field">
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.label :description %><br />
		<%= f.text_field :desc %>
	</div>
	<div class="actions">
		<%= f.submit "Create" %>
	</div>
<%end%>
